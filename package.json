{
	"name": "insertseq",
	"version": "1.0.0",
	"publisher": "volkerdobler",
	"displayName": "Insert Sequences",
	"description": "Insert sequences of any kind (numberic, alpha, expressions, etc.) to VSCode textdocuments",
	"keywords": [
		"sequences",
		"numbers",
		"insert",
		"alphachars",
		"expressions",
		"frequencies",
		"insertnums",
		"insertseq",
		"insertsequences"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkerdobler/insertnums/issues",
		"email": "vscode.insertsequences.ext@volker-dobler.de"
	},
	"homepage": "https://github.com/volkerdobler/insertnums/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/volkerdobler/insertnums.git"
	},
	"engines": {
		"vscode": "^1.88.0"
	},
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"title": "insertseq",
			"properties": {
				"start": {
					"type": "string",
					"default": "1",
					"description": "Default start"
				},
				"step": {
					"type": "string",
					"default": "1",
					"description": "Default increment"
				},
				"frequency": {
					"type": "string",
					"default": "0",
					"description": "Default frequency"
				},
				"repetition": {
					"type": "string",
					"default": "0",
					"description": "Default repetition"
				},
				"stringFormat": {
					"type": "string",
					"default": "",
					"description": "Default output format for strings"
				},
				"numberFormat": {
					"type": "string",
					"default": "",
					"description": "Default output format for numbers"
				},
				"dateFormat": {
					"type": "string",
					"default": "dd.MM.yyyy",
					"description": "Default output format for dates"
				},
				"insertOrder": {
					"type": "string",
					"default": "cursor",
					"description": "Default order of insertion",
					"enum": [
						"cursor",
						"sorted",
						"reverse"
					],
					"enumdescription": [
						"cursor (default): insertion is based on multi cursor click order",
						"sorted: insertion is from top most click down",
						"reverse: insertion is from bottom most click up"
					]
				},
				"language": {
					"type": "string",
					"default": "de",
					"description": "Default language format for dates"
				},
				"century": {
					"type": "string",
					"default": "20",
					"description": "Default century if year has only 2 digits - default: 20"
				},
				"centerString": {
					"type": "string",
					"default": "l",
					"enum": [
						"l",
						"r"
					],
					"enumdescription": [
						"l (default): e.g. a 1 char string centered in a 4 char field is left oriented (placed at position 2)",
						"r: e.g. a 1 char string centered in a 4 char field is right oriented (placed at position 3)"
					]
				},
				"dateStepUnit": {
					"type": "string",
					"default": "d",
					"description": "Default unit as date step",
					"enum": [
						"d",
						"w",
						"m",
						"y"
					],
					"enumdescription": [
						"d (default): days",
						"w: weeks",
						"m: months",
						"y: years"
					]
				},
				"delimiter": {
					"type": "string",
					"default": " ",
					"description": "Delimiter, if no newlines should be inserted"
				},
				"alphabet": {
					"type": "string",
					"default": "abcdefghijklmnopqrstuvwxyz",
					"description": "Alphabet, for string successors"
				},
				"ownsequences": {
					"type": "array",
					"default": [
						[
							"Jan",
							"Feb",
							"Mar",
							"Apr",
							"May",
							"Jun",
							"Jul",
							"Aug",
							"Sep",
							"Oct",
							"Nov",
							"Dec"
						],
						[
							"foo",
							"bar",
							"foz"
						]
					],
					"description": "Own defined Sequences (Array)"
				}
			}
		},
		"commands": [
			{
				"command": "extension.insertSeq",
				"title": "Insert Sequences"
			}
		],
		"keybindings": [
			{
				"command": "extension.insertSeq",
				"key": "ctrl+alt+.",
				"mac": "cmd+alt+.",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"check-types": "tsc --noEmit",
		"lint": "eslint src --ext ts",
		"compile": "tsc -p",
		"pretest": "npm run compile && npm run lint",
		"package": "npm run check-types && node esbuild.js --production",
		"vscode:prepublish": "npm run compile"
	},
	"devDependencies": {
		"@types/assert": "^1.5.11",
		"@types/vscode": "^1.89.0",
		"assert": "^2.1.0",
		"eslint": "^9.2.0",
		"prettier": "^3.2.5"
	},
	"icon": "insertsequences.png",
	"galleryBanner": {
		"color": "#5757ff",
		"theme": "dark"
	}
}
